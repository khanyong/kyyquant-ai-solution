# ===== 키움증권 API 설정 =====
KIWOOM_APP_KEY=your_kiwoom_app_key_here
KIWOOM_APP_SECRET=your_kiwoom_app_secret_here
KIWOOM_ACCOUNT_NO=your_account_number
KIWOOM_IS_DEMO=true  # 모의투자: true, 실전: false

# API URLs
KIWOOM_API_URL=https://mockapi.kiwoom.com
KIWOOM_WS_URL=ws://openapi.kiwoom.com:9443

# ===== Supabase 설정 =====
# Frontend (공개 가능 - RLS로 보호됨)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Backend (절대 공개 금지!)
SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# ===== 로컬 서버 =====
VITE_KIWOOM_WS_URL=ws://localhost:8765

# ===== 백테스트 서버 =====
VITE_API_URL=http://localhost:8080

# ===== N8N (선택사항) =====
VITE_N8N_URL=http://localhost:5679
VITE_N8N_API_KEY=your_n8n_api_key

# ===== GitHub (선택사항) =====
GITHUB_TOKEN=your_github_personal_access_token

# ===== 거래 설정 =====
AUTO_LOGIN=false
DEMO_MODE=true
SCREEN_NO_BASE=1000
REQUEST_DELAY_MS=200
