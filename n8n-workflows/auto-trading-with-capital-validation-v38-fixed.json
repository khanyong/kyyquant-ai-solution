      "parameters": {
        "jsCode": "// ëª¨ë“  ?…ë ¥ ?„ì´??ì²˜ë¦¬\nconst mergedData = $('?ë³¸ ?°ì´??ë³‘í•©').all();\nconst stockNameResults = $('ì¢…ëª©ëª?ì¡°íšŒ').all();\nconst prevCloseResults = $('?„ì¼ì¢…ê? ì¡°íšŒ').all();\n\nconst results = [];\n\nfor (let i = 0; i < mergedData.length; i++) {\n  const stockData = mergedData[i].json;\n  const kiwoomPrice = stockData.kiwoom_data;\n  \n  // n8n HTTP Request ?¸ë“œ??Supabase ë°°ì—´ ?‘ë‹µ??ê°œë³„ ?„ì´?œìœ¼ë¡?ë¶„ë¦¬??n  const stockNameData = stockNameResults[i]?.json;\n  const prevCloseData = prevCloseResults[i]?.json;\n\n  // ?”ë²„ê¹?n  console.log(`[${i}] stockNameData:`, JSON.stringify(stockNameData));\n  \n  // HTTP Request ?¸ë“œê°€ ë°°ì—´???€?´ì„œ ë°˜í™˜?˜ë?ë¡?ì§ì ‘ ?‘ê·¼\n  let stockName = stockData.stock_code; // ê¸°ë³¸ê°?n  \n  if (stockNameData && stockNameData.stock_name) {\n    stockName = stockNameData.stock_name;\n    console.log(`[${i}] Found stock_name: ${stockName}`);\n  } else {\n    console.log(`[${i}] stockNameData not found or invalid:`, JSON.stringify(stockNameData));\n  }\n\n  const previousClose = (prevCloseData && prevCloseData.close) \n    ? parseFloat(prevCloseData.close) \n    : 0;\n\n  const parsePrice = (price) => {\n    if (!price) return 0;\n    return parseFloat(String(price).replace(/[+\\-]/g, ''));\n  };\n\n  const selPrice = parsePrice(kiwoomPrice.sel_fpr_bid);\n  const buyPrice = parsePrice(kiwoomPrice.buy_fpr_bid);\n  const estimatedPrice = (selPrice + buyPrice) / 2;\n\n  // ? ï¸ ê°€ê²?ê²€ì¦? current_priceê°€ 0?´ê±°??change_rateê°€ -100%?´ë©´ ?€???¤í‚µ\n  if (estimatedPrice === 0 || selPrice === 0 || buyPrice === 0) {\n    console.warn(`? ï¸ [${i + 1}/${mergedData.length}] ${stockData.stock_code}: ?˜ëª»??ê°€ê²??°ì´??(current_price: ${estimatedPrice}) - ?€???¤í‚µ`);\n    continue; // ??ì¢…ëª©?€ ê±´ë„ˆ?°ê¸°\n  }\n\n  const changePrice = previousClose > 0 ? estimatedPrice - previousClose : 0;\n  const changeRate = previousClose > 0 ? ((changePrice / previousClose) * 100) : 0;\n\n  // change_rateê°€ -100%ë©?API ?¤ë¥˜?´ë?ë¡??€???¤í‚µ\n  if (changeRate === -100 || changeRate < -50) {\n    console.warn(`? ï¸ [${i + 1}/${mergedData.length}] ${stockData.stock_code}: ë¹„ì •???±ë½ë¥?(${changeRate.toFixed(2)}%) - ?€???¤í‚µ`);\n    continue;\n  }\n\n  const selVolume = parseInt(String(kiwoomPrice.sel_fpr_req || 0).replace(/[+\\-]/g, '')) || 0;\n  const buyVolume = parseInt(String(kiwoomPrice.buy_fpr_req || 0).replace(/[+\\-]/g, '')) || 0;\n\n  const result = {\n    stock_code: stockData.stock_code,\n    stock_name: stockName,\n    current_price: estimatedPrice,\n    change_price: changePrice,\n    change_rate: changeRate,\n    volume: selVolume + buyVolume,\n    sel_price: selPrice,\n    buy_price: buyPrice,\n    SUPABASE_URL: stockData.SUPABASE_URL,\n    SUPABASE_SERVICE_ROLE_KEY: stockData.SUPABASE_SERVICE_ROLE_KEY,\n    index: i,\n    total: mergedData.length\n  };\n\n  console.log(`??[${i + 1}/${mergedData.length}] ${result.stock_code} (${result.stock_name}): ${result.current_price.toLocaleString()}??(${result.change_rate > 0 ? '+' : ''}${result.change_rate.toFixed(2)}%)`);\n\n  results.push({ json: result });\n}\n\nif (results.length === 0) {\n  console.warn('? ï¸ ?€?¥í•  ? íš¨??ê°€ê²??°ì´?°ê? ?†ìŠµ?ˆë‹¤ (ëª¨ë“  ì¢…ëª© ?¤í‚µ??');\n}\n\nreturn results;"
